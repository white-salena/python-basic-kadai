    "    def __init__(self, name, age):\n",
    "        self.name = name\n",
    "        self.age = age\n",
    "\n",
    "    def printinfo(self):\n",
    "        print(self.name)\n",
    "        print(self.age)\n",
    "\n",
    "man = Human(\"侍太郎\", 37)\n",
    "\n",
    "man.printinfo()"
